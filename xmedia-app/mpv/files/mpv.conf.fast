ontop=yes
save-position-on-quit
af=lavfi=[firequalizer=gain='if(lt(f,400), -30, 0)',acompressor=threshold=-20dB:ratio=3:attack=50:release=200]
geometry=33%x33%-0-0
reset-on-next-file=audio-delay,mute,pause,speed,sub-delay,video-aspect-override,video-pan-x,video-pan-y,video-rotate,video-zoom,volume
border=no
cursor-autohide=500
hwdec-codecs=all
hwdec=auto
no-cache-pause
focus=no

ytdl-format="137+140/299+140/bestvideo[height<=?1080][vcodec!=?vp9]+bestaudio/best"
